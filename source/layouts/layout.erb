    <%= stylesheet_link_tag "normalize", "nhnm_landingpages_2015_q2", "nhnm_forms_2015_q2" %>

    <%= partial "head_nhnm_eq_tracking" %>  
    <%= partial "head_nhnm_eq_wdl" %>  

